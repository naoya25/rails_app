<div class="new-all">
  <h3>新規投稿</h3>
  <%= form_for @instagram do |f| %>
    <div class="field">
      <%= f.label :body %>
      <%= f.text_field :body, :size => 140 %>
    </div>
    <div class="field">
      <%= f.label :image %>
      <%= f.file_field :image %>
    </div>
    <%= f.submit "共有する" %>
  <% end %>
  <%= link_to "投稿一覧に戻る", instagrams_path %>
</div>